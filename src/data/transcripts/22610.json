    {
        "text": "We were supposed to protect them.",
        "startTime": 127506,
        "endTime": 129447,
        "slowStartTime": 191409,
        "slowEndTime": 194396
    },
    {
        "text": "You're afraid.",
        "startTime": 133780,
        "endTime": 134869,
        "slowStartTime": 200820,
        "slowEndTime": 202528
    },
    {
        "text": "I remember.",
        "startTime": 135596,
