    {
        "text": "Just pack a bag and a camera.",
        "startTime": 61040,
        "endTime": 62613,
        "slowStartTime": 91710,
        "slowEndTime": 94144
    },
    {
        "text": "Get lost.",
        "startTime": 63071,
        "endTime": 63803,
        "slowStartTime": 94756,
        "slowEndTime": 95929
    },
    {
        "text": "So, where are you gonna go?",
        "startTime": 65307,
