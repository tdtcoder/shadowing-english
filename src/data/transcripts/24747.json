    {
        "text": "You get it?",
        "startTime": 33047,
        "endTime": 33371,
        "slowStartTime": 49721,
        "slowEndTime": 50281
    },
    {
        "text": "Handy, hands?",
        "startTime": 33652,
        "endTime": 35014,
        "slowStartTime": 50628,
        "slowEndTime": 52746
    },
    {
        "text": "Sorry.",
        "startTime": 35617,
        "endTime": 35870,
        "slowStartTime": 53575,
        "slowEndTime": 54030
    },
    {
