[
    {
        "text": "Antonio was a wealthy merchant in the Italian city of Venice.",
        "startTime": 2001,
        "endTime": 6022,
        "slowStartTime": 3152,
        "slowEndTime": 9258
    },
    {
        "text": "His friend, Bassanio, needed money to impress Portia, a wealthy heiress.",
        "startTime": 6920,
        "endTime": 12237,
        "slowStartTime": 10530,
        "slowEndTime": 18581
    },
    {
        "text": "Antonio could not make the loan himself...",
        "startTime": 13369,
        "endTime": 15730,
        "slowStartTime": 20203,
        "slowEndTime": 23820
