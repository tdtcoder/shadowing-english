[
    {
        "text": "I hope this letter finds you well.",
        "startTime": 1473,
        "endTime": 2903,
        "slowStartTime": 2360,
        "slowEndTime": 4579
    },
    {
        "text": "I was looking at some pictures yesterday and it made me think of you.",
        "startTime": 3426,
