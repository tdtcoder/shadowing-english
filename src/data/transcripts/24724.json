[
    {
        "text": "In the beginning, Jim was a small town player.",
        "startTime": 1118,
        "endTime": 4454,
        "slowStartTime": 1827,
        "slowEndTime": 6906
    },
    {
        "text": "He's a very smart, disciplined man.",
        "startTime": 5503,
        "endTime": 8248,
        "slowStartTime": 8404,
        "slowEndTime": 12597
    },
    {
        "text": "Take your shot.",
        "startTime": 8577,
        "endTime": 9372,
        "slowStartTime": 13016,
        "slowEndTime": 14283
    },
    {
        "text": "But make it your best, cause I get up, I eat you.",
