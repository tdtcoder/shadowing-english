[
    {
        "text": "Meet fashion model Han Hyun-min.",
        "startTime": 793,
        "endTime": 3173,
        "slowStartTime": 1340,
        "slowEndTime": 4985
    },
    {
        "text": "He's young, gifted and black,...",
        "startTime": 3925,
