    {
        "text": "This video will help you choose among the best of the best.",
        "startTime": 12225,
        "endTime": 14943,
        "slowStartTime": 18487,
        "slowEndTime": 22640
    },
    {
        "text": "Greece is a great summer holiday location with beautiful islands...",
        "startTime": 15927,
        "endTime": 19467,
        "slowStartTime": 24040,
        "slowEndTime": 29425
