[
    {
        "text": "Scarlett O'Hara lived on a plantation in Georgia, called Tara.",
        "startTime": 2371,
        "endTime": 7423,
        "slowStartTime": 3707,
        "slowEndTime": 11359
    },
    {
        "text": "She met Rhett Butler, a dashing adventurer.",
        "startTime": 8218,
        "endTime": 11541,
        "slowStartTime": 12477,
        "slowEndTime": 17536
    },
    {
        "text": "The Civil War began.",
        "startTime": 12233,
        "endTime": 13841,
        "slowStartTime": 18500,
        "slowEndTime": 20987
    },
    {
        "text": "Scarlett married Charles Hamilton, hoping to hurt a man she once loved.",
        "startTime": 14607,
        "endTime": 19874,
        "slowStartTime": 22061,
        "slowEndTime": 30036
    },
    {
        "text": "Charles went to war and died of measles.",
        "startTime": 21004,
        "endTime": 24124,
        "slowStartTime": 31656,
        "slowEndTime": 36411
    },
    {
        "text": "She had his son and named him Wade.",
        "startTime": 25031,
        "endTime": 27669,
        "slowStartTime": 37697,
        "slowEndTime": 41729
    },
    {
        "text": "They moved to Atlanta to live with Charles' sister Melanie.",
        "startTime": 28537,
        "endTime": 32866,
        "slowStartTime": 42956,
        "slowEndTime": 49524
    },
    {
        "text": "Scarlett saw Rhett a lot and they developed a stormy relationship.",
        "startTime": 33822,
        "endTime": 39029,
        "slowStartTime": 50883,
        "slowEndTime": 58769
    },
    {
        "text": "Melanie's husband Ashley was captured in the war.",
        "startTime": 39960,
        "endTime": 43495,
        "slowStartTime": 60090,
        "slowEndTime": 65467
    },
    {
        "text": "Melanie was pregnant and Scarlett agreed to help her.",
        "startTime": 44455,
        "endTime": 48385,
        "slowStartTime": 66833,
        "slowEndTime": 72803
    },
    {
        "text": "Rhett helped Scarlett, Melanie and her baby, Beau, escape.",
        "startTime": 49435,
        "endTime": 53695,
        "slowStartTime": 74303,
        "slowEndTime": 80767
    },
    {
        "text": "He left them to join the Confederate army.",
        "startTime": 54499,
        "endTime": 57213,
        "slowStartTime": 81898,
        "slowEndTime": 86045
    },
    {
        "text": "The war ended and Scarlett rebuilt Tara.",
        "startTime": 60095,
        "endTime": 63405,
        "slowStartTime": 90293,
        "slowEndTime": 95333
    },
    {
        "text": "Scarlett married Frank and they had a baby girl, Ella.",
        "startTime": 64293,
        "endTime": 68521,
        "slowStartTime": 96589,
        "slowEndTime": 103007
    },
    {
        "text": "She borrowed money from Rhett and bought a sawmill.",
        "startTime": 69439,
        "endTime": 73059,
        "slowStartTime": 104309,
        "slowEndTime": 109813
    },
    {
        "text": "Frank was killed by the Ku Klux Klan.",
        "startTime": 74133,
        "endTime": 76924,
        "slowStartTime": 111349,
        "slowEndTime": 115611
    },
    {
        "text": "Scarlett married Rhett after all and they moved to Atlanta.",
        "startTime": 77823,
        "endTime": 82391,
        "slowStartTime": 116884,
        "slowEndTime": 123811
    },
    {
        "text": "They had a daughter named Bonnie.",
        "startTime": 83150,
        "endTime": 85064,
        "slowStartTime": 124875,
        "slowEndTime": 127821
    },
    {
        "text": "Scarlett and Rhett's marriage began happily,...",
        "startTime": 85964,
        "endTime": 88987,
        "slowStartTime": 129096,
        "slowEndTime": 133705
    },
    {
        "text": "...but Rhett became increasingly bitter and indifferent toward her.",
        "startTime": 89611,
        "endTime": 94095,
        "slowStartTime": 134566,
        "slowEndTime": 141367
    },
    {
        "text": "Their marriage began to fall apart.",
        "startTime": 95032,
        "endTime": 97369,
        "slowStartTime": 142698,
        "slowEndTime": 146278
    },
    {
        "text": "During that time, Bonnie was killed when she fell off of a horse.",
        "startTime": 98369,
        "endTime": 103299,
        "slowStartTime": 147704,
        "slowEndTime": 155173
    },
    {
        "text": "Scarlett tried to save their marriage but Rhett left her.",
        "startTime": 104171,
        "endTime": 108380,
        "slowStartTime": 156406,
        "slowEndTime": 162795
    },
    {
        "text": "She ended up moving back to Tara.",
        "startTime": 109240,
        "endTime": 111535,
        "slowStartTime": 164010,
        "slowEndTime": 167528
    }
]