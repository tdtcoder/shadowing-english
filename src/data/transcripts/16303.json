[
    {
        "text": "One of the least traveled countries is Mauritania.",
        "startTime": 2135,
        "endTime": 5296,
        "slowStartTime": 3352,
        "slowEndTime": 8169
    },
    {
        "text": "This is not a place to visit if you are hung up on comfort and luxury,...",
        "startTime": 5943,
