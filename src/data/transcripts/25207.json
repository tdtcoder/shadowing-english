        "slowStartTime": 39026,
        "slowEndTime": 46277
    },
    {
        "text": "I'm Matt Rush.",
        "startTime": 35444,
        "endTime": 36195,
        "slowStartTime": 53316,
        "slowEndTime": 54517
    },
    {
        "text": "I just graduated from Olin College.",
        "startTime": 36400,
        "endTime": 38401,
        "slowStartTime": 54750,
        "slowEndTime": 57826
    },
    {
