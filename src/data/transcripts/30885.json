    {
        "text": "...you can enjoy your meal and there's a button to call the server.",
        "startTime": 55851,
        "endTime": 58511,
        "slowStartTime": 83926,
        "slowEndTime": 87992
    },
    {
        "text": "The Mexican restaurant began using the tablets a few weeks ago...",
        "startTime": 58624,
        "endTime": 61722,
        "slowStartTime": 88086,
        "slowEndTime": 92808
