        "endTime": 45432,
        "slowStartTime": 66628,
        "slowEndTime": 68373
    },
    {
        "text": "Good!",
        "startTime": 46257,
        "endTime": 46647,
        "slowStartTime": 69535,
        "slowEndTime": 70196
    },
    {
