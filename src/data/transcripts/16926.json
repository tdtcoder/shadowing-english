    {
        "text": "Um, how about trading clothes with each other?",
        "startTime": 104527,
        "endTime": 108609,
        "slowStartTime": 156940,
        "slowEndTime": 163139
    },
    {
        "text": "Wow, I can't believe it! You look just like me!",
        "startTime": 109808,
        "endTime": 114346,
        "slowStartTime": 164862,
        "slowEndTime": 171744
