[
    {
        "text": "Is your job about to get automated?",
        "startTime": 50,
        "endTime": 1731,
        "slowStartTime": 175,
        "slowEndTime": 2821
    },
    {
        "text": "Yes, probably.",
        "startTime": 2035,
