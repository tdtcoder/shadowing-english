[
    {
        "text": "Hi, I'm Bella Thorne and I've been bullied my whole life.",
        "startTime": 1157,
        "endTime": 4587,
        "slowStartTime": 1885,
        "slowEndTime": 7105
    },
    {
        "text": "It started when I was about five or six and it was before school...",
        "startTime": 4756,
        "endTime": 8313,
        "slowStartTime": 7284,
        "slowEndTime": 12695
    },
    {
        "text": "...because I spoke Spanish, that's my first language,...",
        "startTime": 8651,
        "endTime": 11811,
        "slowStartTime": 13126,
        "slowEndTime": 17941
