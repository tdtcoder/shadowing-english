[
    {
        "text": "Well, my chess career really began in California.",
        "startTime": 4600,
        "endTime": 7815,
        "slowStartTime": 7050,
        "slowEndTime": 11948
    },
    {
        "text": "My brother was a junior player when he was quite young...",
        "startTime": 7890,
        "endTime": 10929,
        "slowStartTime": 11985,
        "slowEndTime": 16619
