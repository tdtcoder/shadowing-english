[
    {
        "text": "Dear Spike, Dear Oprah, Dear Stevie...",
        "startTime": 261,
        "endTime": 4087,
        "slowStartTime": 542,
        "slowEndTime": 6281
    },
    {
        "text": "Your music has guided me through the most difficult moments of my life.",
        "startTime": 4379,
        "endTime": 8046,
        "slowStartTime": 6719,
        "slowEndTime": 12219
    },
    {
        "text": "I get letters from so many people that share their stories.",
        "startTime": 8638,
        "endTime": 12557,
        "slowStartTime": 13107,
        "slowEndTime": 18986
    },
    {
        "text": "I keep fighting voices in my mind...",
