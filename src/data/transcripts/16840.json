[
    {
        "text": "A long long time ago, a poor widow lived with her son named Jack.",
        "startTime": 875,
        "endTime": 5459,
        "slowStartTime": 1463,
        "slowEndTime": 8414
    },
    {
        "text": "She had hard times because she was very poor.",
        "startTime": 6553,
        "endTime": 9670,
        "slowStartTime": 9979,
        "slowEndTime": 14730
    },
    {
        "text": "One day, she called her son.",
        "startTime": 10755,
        "endTime": 12701,
        "slowStartTime": 16283,
        "slowEndTime": 19277
    },
    {
        "text": "Jack, come here.",
        "startTime": 13709,
        "endTime": 15941,
        "slowStartTime": 20714,
        "slowEndTime": 24136
    },
    {
        "text": "Jack, listen carefully.",
        "startTime": 17151,
        "endTime": 18905,
        "slowStartTime": 25877,
        "slowEndTime": 28582
    },
    {
        "text": "We have to sell Milky White.",
        "startTime": 20019,
        "endTime": 21878,
        "slowStartTime": 30178,
        "slowEndTime": 33042
    },
    {
        "text": "Go and sell her at the market.",
        "startTime": 22992,
        "endTime": 24762,
        "slowStartTime": 34638,
        "slowEndTime": 37368
